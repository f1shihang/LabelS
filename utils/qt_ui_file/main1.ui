           <property name="styleSheet">
            <string notr="true">/* 设置QLineEdit的风格 */
QLineEdit {
    border: 2px solid rgb(210, 210, 210); /* 设置边框为较浅的灰色 */
    border-radius: 10px; /* 圆角度数 */
    padding: 0 8px; /* 内边距 */
    background-color: white; /* 背景颜色设置为白色 */
    /* 可以添加一个简单的渐变效果 */
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #ffffff, stop:1 #e6e6e6);
    /* 轻微的内阴影效果 */
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1) inset;
}

/* 当QLineEdit部件获得焦点时 */
QLineEdit:focus {
    /* 边框颜色加深，突出显示 */
    border: 2px solid rgb(66, 133, 244); /* 蓝色高亮 */
    /* 添加更明显的内阴影以显示焦点状态 */
    box-shadow: 0 0 3px rgba(66, 133, 244, 0.5) inset;
}

/* 设置默认边框颜色 */
border: 1px solid rgb(210, 210, 210); /* 淡灰色边框 */
